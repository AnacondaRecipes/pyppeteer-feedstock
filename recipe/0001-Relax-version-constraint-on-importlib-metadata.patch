@@ -46,7 +46,7 @@ pyppeteer-install = 'pyppeteer.command:install'
 [tool.poetry.dependencies]
 python = "^3.6.1"
 appdirs = "^1.4.3"
-importlib-metadata = ">=1.4"
+importlib-metadata = { version = ">=2.1.1", python = "<3.8" }
 pyee = "^8.1.0"
 tqdm = "^4.42.1"
 urllib3 = "^1.25.8"
